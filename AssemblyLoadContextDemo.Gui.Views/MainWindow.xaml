<Window x:Class="AssemblyLoadContextDemo.Gui.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssemblyLoadContextDemo.Gui.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Content="Load" Command="{Binding Load}"/>
            <Button Content="Unload" Command="{Binding Unload}"/>
            <Button Content="Collect Garbage" Command="{Binding CollectGarbage}"/>
            <Label Content="{Binding result}"/>
        </StackPanel>

    </Grid>
</Window>
